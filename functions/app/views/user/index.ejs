<% include ../header %>

<div class="container">
    <h2>Users</h2>
    <a type="button" class="btn btn-primary" href=<%= app_name %>/user/new>New</a>
    <hr>
    <table class="table">
        <thead>
        <tr>
            <th>ID</th>
            <th>Username</th>
            <th>Email</th>
        </tr>
        </thead>
        <tbody>
        <% users.forEach(function(user){ %>
        <tr>
            <td><%= user.id %></td>
            <td><%= user.user_name %></td>
            <td><%= user.email %></td>

        </tr>
        <% }); %>
        </tbody>
    </table>
</div>